{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier", "import", "jest"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "curly": "warn"
  }
}
